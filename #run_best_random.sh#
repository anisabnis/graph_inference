`````#!/bin/bash

networks=(sinet sanet tata evolink rnp)
intnodes=(1 1 3 1 1)

for j in {0..0}; do
    python gen_rd_init.py ${networks[${j}]}
    for i in {1..4}; do
	./script_random_init.sh ${networks[${j}]} ${intnodes[${j}]} ${i}
    done
    python gen_tree_exp3.py ${networks[${j}]} 4
    for i in {1..4}; do
	./script_random.sh ${networks[${j}]} ${intnodes[${j}]} ${i}
    done
done