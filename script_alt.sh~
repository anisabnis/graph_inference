#!/bin/bash

dir=$1
enc=$2
ntrees=$3

mkdir results_alt_trees/$dir$ntrees/

cp results/$dir/graph.txt results_alt_trees/$dir$ntrees/
cp results/$dir/distances.txt results_alt_trees/$dir$ntrees/
cp results/$dir/orig_path.txt results_alt_trees/$dir$ntrees/

python graph-opt.py results_alt_trees/$dir$ntrees $enc $ntrees

gams tree.gams

scp -2 tree.mps asabnis@users.deterlab.net:/users/asabnis/cplex/bin/x86-64_linux/
ssh -2 asabnis@ct1.h6e-t1a.edgect.isi.deterlab.net "rm ~/cplex/bin/x86-64_linux/solution.xml"
ssh -2 asabnis@ct1.h6e-t1a.edgect.isi.deterlab.net "cd ~/cplex/bin/x86-64_linux/; ./cplex -f ./command.txt; cd ../../..;" 
scp -2 asabnis@ct1.h6e-t1a.edgect.isi.deterlab.net:~/cplex/bin/x86-64_linux/solution.xml results_alt_trees/$dir$ntrees/res$enc.xml

python parsetrees.py results_alt_trees/$dir$ntrees $enc
python parse_solution.py results_alt_trees/$dir$ntrees 

python gen_dot1.py results_alt_trees/$dir$ntrees
fdp results_alt_trees/$dir$ntrees/orig.dot -Tpng -o results_alt_trees/$dir$ntrees/original.png
fdp results_alt_trees/$dir$ntrees/inf.dot -Tpng -o results_alt_trees/$dir$ntrees/inferred.png

./gedevo.sh results_alt_trees/$dir$ntrees/